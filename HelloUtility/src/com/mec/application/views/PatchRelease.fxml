<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<GridPane prefHeight="550.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mec.application.views.PatchReleaseController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="1.7976931348623157E308" prefHeight="185.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <TabPane>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
         <tabs>
            <Tab closable="false" text="Incremental Package">
               <content>
                  <GridPane maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="360.0" prefWidth="650.0">
                    <columnConstraints>
                      <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="150.0" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="494.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="250.0" vgrow="SOMETIMES" />
                      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="EE ID" />
                        <Label text="Workspace Directory" GridPane.rowIndex="1" />
                        <Label text="Patch Release Directory" GridPane.rowIndex="3" />
                        <Label text="Modify List (README)" GridPane.rowIndex="2" />
                        <TextArea fx:id="modifyList" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <TextField fx:id="patchReleaseDirectory" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <TextField fx:id="workSpaceDirectory" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField disable="true" prefWidth="447.0" promptText="Input EE-ID into Modfiy List as comment (starts with --)" GridPane.columnIndex="1" />
                        <HBox spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <children>
                              <Button fx:id="startPatch" mnemonicParsing="false" onAction="#onStartPatch" text="Try Patch" />
                              <Button fx:id="clearLogBtn" mnemonicParsing="false" onAction="#onClearLog" text="Clear Log" />
                           </children>
                           <padding>
                              <Insets top="5.0" />
                           </padding>
                        </HBox>
                     </children>
                  </GridPane>
               </content>
            </Tab>
            <Tab closable="false" text="Full Package">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
         </tabs>
      </TabPane>
      <TextArea fx:id="logMsg" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" promptText="Log Message" GridPane.columnSpan="2" GridPane.rowIndex="1" />
   </children>
</GridPane>
