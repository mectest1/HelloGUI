<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<GridPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mec.application.views.BaseCodeDecoderController">
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="500.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="130.0" />
      <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="500.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <TextArea fx:id="encodedText" prefHeight="600.0" prefWidth="500.0" />
      <VBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1">
         <children>
            <Button fx:id="decodeGzippedButton" defaultButton="true" mnemonicParsing="false" onAction="#onDecodeBase64Gzip" text="Decode GZipped &gt;&gt;" />
            <Button fx:id="decodeBase64Button" mnemonicParsing="false" onAction="#onDecodeBase64" text="Decode Base64 &gt;&gt;" />
            <Button fx:id="encodeBase64Button" mnemonicParsing="false" onAction="#onEncodBase64" text="&lt;&lt; Encode Base64" />
            <Button fx:id="encodeGzippedButton" mnemonicParsing="false" onAction="#onEncodeBase64Gzip" text="&lt;&lt; Encode Gzipped" />
         </children>
      </VBox>
      <TextArea fx:id="decodedText" prefHeight="600.0" prefWidth="500.0" GridPane.columnIndex="2" />
   </children>
   <stylesheets>
      <URL value="@viewStyle.css" />
   </stylesheets>
</GridPane>
